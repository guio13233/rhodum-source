				<ProtectedString name="Source">function badiedetected(badie)
if badie.Name == &quot;HealthGUI&quot; then
badie:remove()
script:remove()
end
end

local badie = script.Parent.Parent:FindFirstChild(&quot;HealthGUI&quot;)
if badie ~= nil then
badie:remove()
script:remove()
